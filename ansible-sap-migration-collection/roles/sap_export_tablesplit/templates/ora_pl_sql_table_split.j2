# Oracle only: Select type of splitter. Possible values are: 'R3TA', 'PLSQL'
# TableSplitter_ora.splitter = PLSQL

# Windows only: The domain of the SAP Host Agent user
# hostAgent.domain = 

# Password for the 'sapadm' user of the SAP Host Agent
