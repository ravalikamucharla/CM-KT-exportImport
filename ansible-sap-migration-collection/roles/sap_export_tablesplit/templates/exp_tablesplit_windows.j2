# Specify whether you want to decluster during the export.
InitDeclusteringForExport.decluster = false

# Specify whether Software Provisioning Manager is to create ('CRE') a new database or re-use ('USE') an existing database.
MSS.Database.Action = USE

# Target database type, possible values are: 'ADA', 'HDB', 'DB2', 'DB4', 'DB6', 'MSS', 'ORA', 'SYB'
NW_ABAP_Export_Dialog.targetDbType = {{ params.import.target_db_type }}

# UNIX only: If this parameter is set to 'RESTRICTED' the export subdirectories will belong to '<sapsid>adm:sapsys' with permission 700. Leave empty for default.
# NW_Export.accessLevel4ExportDir = default

# Main export directory with ABAP and JAVA subdirectories
NW_Export.mainExportDir = {{ params.export.mainExportDir }}

# File with predefined tables to be split
NW_Export.tableFile = C:\Users\admin_accenture\Desktop\tablesplitting.txt

# Database host
NW_MSS_DBINFO.dbhost = {{ hostname |upper }}

# Database system ID (DBSID)
NW_MSS_DBINFO.dbsid = {{ sid|upper }}

# The FQDN of the system
# NW_getFQDN.FQDN = 

# SAP INTERNAL USE ONLY
# NW_getFQDN.resolve = true

# Specify whether you want to set FQDN for the system.
# NW_getFQDN.setFQDN = true

# The load type chosen by the user. Valid values are: 'SAP', 'STD', 'OBR', 'HCP', 'FLASHCOPY', 'MDA', 'HBR', 'SBR'
