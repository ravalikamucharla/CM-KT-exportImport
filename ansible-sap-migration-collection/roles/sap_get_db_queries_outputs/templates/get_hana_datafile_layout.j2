[{% for data in output.results[index].stdout_lines %}
    {% if loop.length == 1 %}
        {
            "HOST": "",
            "FILE_NAME": ""
        }
    {% elif loop.index > 1 and loop.index <= loop.length-1 %}
        {
            "HOST": {{ data.split(',')[0] }},
            "FILE_NAME": {{ data.split(',')[1] }}
        },
    {% elif loop.index == loop.length %}
        {
            "HOST": {{ data.split(',')[0] }},
            "FILE_NAME": {{ data.split(',')[1] }}
        }
    {% endif %}
{% endfor %}]
